{
  "$schema": "https://weme-ui.github.io/schema/registry.json",
  "name": "@{{ owner }}/{{ name }}",
  "description": "Weme UI for {{ titleCase name }} components registry.",
  "version": "{{ version }}",
  "prefix": "{{ name }}",
  "directory": "registry/{{ name }}",
  "access": "public",
  "meta": {
    "authors": [
      "{{{ author }}}"
    ],
    "homepage": "{{ homepage }}",
    "repository": "{{ erase repository.url "git+" }}",
    "bugs": "{{ bugs.url }}",
    "tags": [
      "@{{ owner }}/{{ name }}"
    ]
  },
  "dependencies": [
    "clsx",
    "tailwind-merge",
    "tailwind-variants"
  ],
  "devDependencies": [
    "@weme-ui/nuxt",
    "@weme-ui/unocss-preset"
  ],
  "items": [
  ]
}
