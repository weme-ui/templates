{
  "$schema": "https://weme-ui.github.io/schema/registry.json",
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "prefix": "{{ prefix }}",
  "directory": "{{ directory }}",
  "access": "{{ access }}",
  "meta": {
    "authors": [
      "{{ author }}"
    ],
    "homepage": "{{ homepage }}",
    "repository": "{{ github }}",
    "bugs": "{{ github }}/issues"
  },
  "items": [
    {
      "name": "#init",
      "type": "init",
      "title": "Init",
      "description": "Init dependencies and utils for Weme UI.",
      "devDependencies": [
        "@vueuse/core",
        "@weme-ui/nuxt",
        "@weme-ui/unocss-preset",
        "clsx",
        "nuxt",
        "reka-ui",
        "tailwind-merge",
        "tailwind-variants"
        "unocss",
        "vue-tsc",
        "vue"
      ],
      "files": [
        { "type": "util", "path": "utils/styles.ts" },
        { "type": "util", "path": "utils/props.ts" },
        { "type": "util", "path": "utils/slots.ts" }
      ]
    }
  ]
}
