{
  "$schema": "https://weme-ui.github.io/schema/registry.json",
  "name": "weme-ui/{{ kebabCase name }}",
  "description": "The {{ name }} components for Weme UI.",
  "version": "0.0.0",
  "prefix": "ui",
  "directory": "registry/{{ kebabCase name }}",
  "access": "public",
  "meta": {
    "authors": [
      "Allen Luo <luoyi@mouji.net>"
    ],
    "homepage": "https://github.com/weme-ui/weme-ui",
    "repository": "https://github.com/weme-ui/weme-ui",
    "bugs": "https://github.com/weme-ui/weme-ui/issues",
    "tags": [
      "weme-ui"
    ]
  },
  "items": [
    {
      "name": "#init",
      "type": "init",
      "title": "Init",
      "description": "Init dependencies and utils for Weme UI.",
      "dependencies": [
        "clsx",
        "tailwind-merge",
        "tailwind-variants"
      ],
      "devDependencies": [
        "@nuxt/icon",
        "@internationalized/date",
        "@unocss/nuxt",
        "@vueuse/core",
        "@weme-ui/nuxt",
        "@weme-ui/unocss-preset",
        "reka-ui",
        "unocss",
        "vue"
      ],
      "files": [
        { "type": "util", "path": "utils/styles.ts" },
        { "type": "util", "path": "utils/props.ts" },
        { "type": "util", "path": "utils/slots.ts" }
      ]
    }
  ]
}
